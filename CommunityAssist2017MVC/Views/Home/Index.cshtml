@model IEnumerable<CommunityAssist2017MVC.Models.GrantType>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<table class="table">
    <tr>

        <th>@Html.DisplayNameFor(model => model.GrantTypeKey)</th>
        <th>@Html.DisplayNameFor(model => model.GrantTypeName)</th>
    </tr>
    @foreach (var item in Model)

    {

        <tr>

            <th>@Html.DisplayFor(model => item.GrantTypeKey)</th>
            <th>@Html.DisplayFor(model => item.GrantTypeName)</th>
        </tr>

    }
</table>